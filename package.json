{
  "name": "expressapitest",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/app/server.ts",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist && rimraf node_modules",
    "start": "node dist/app/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "license:check": "npx --yes license-checker --csv --excludePrivatePackages > licensecheck/licenses.csv && node licensecheck/license-report.js",
    "verify": "npm run format:check && npm run lint && npm run build && npm run test && npm run license:check"
  },
  "dependencies": {
    "dotenv-flow": "^3.3.0",
    "express": "^5.2.1",
    "inversify": "^7.10.8",
    "morgan": "^1.10.1",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
